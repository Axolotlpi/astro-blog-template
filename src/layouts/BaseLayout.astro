---
  import BaseHead from '../compositions/BaseHead.astro';
  const headProps = Astro.props;
---
<!DOCTYPE html>
<html lang="en">
  
    <slot name="head">
      <!-- Default head. Note that it won't have any props -->
      <head>
        <BaseHead {...headProps} />
      </head>
    </slot>
  <body>
      <main>

          <slot/>
      
      </main>
  </body>
</html>

