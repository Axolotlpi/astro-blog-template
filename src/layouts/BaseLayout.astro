---
  import BaseHead from '../compositions/BaseHead.astro';
  const headProps = Astro.props;
---
<!DOCTYPE html>
<html lang="en">
  
    <slot name="head">
      <head>
        <BaseHead {...headProps} />
      </head>
    </slot>
  <body>
      <main>
        <slot/>
      	<style global>
		    	@import "../styles/global.css";
        </style>
      </main>
  </body>
</html>

